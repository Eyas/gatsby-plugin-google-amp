{
  "name": "gatsby-remark-amp",
  "version": "1.0",
  "description": "A gatsby plugin for AMP pages",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "commit": "git add . && git commit && git push -u origin master"
  },
  "author": "Mukul Rathi",
  "license": "MIT",
  "homepage": "https://github.com/mukul-rathi/gatsby-plugin-amp#readme",
  "repository": "https://github.com/mukul-rathi/gatsby-plugin-amp",
  "bugs": {
    "url": "https://github.com/mukul-rathi/gatsby-plugin-amp/issues"
  },
  "dependencies": {
    "jsdom": "^13.0.0",
    "lodash.flattendeep": "^4.4.0",
    "minimatch": "^3.0.4",
    "react": "^16.6.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-amp",
    "gatsby-remark-amp",
    "amp",
    "accelerated-mobile-pages"
  ],
  "devDependencies": {
    "babel-preset-gatsby-package": "^0.1.2",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5"
  }
}
